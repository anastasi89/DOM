{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-console": 0,
    "max-len": [
      "error",
      { "code": 150 }
    ],
    "class-methods-use-this": 0,
    "no-alert": 0,
    "no-param-reassign": 0
  }
}